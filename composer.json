{
    "name": "silverstripe/recipe-kitchen-sink",
    "type": "silverstripe-recipe",
    "description": "Internal testing Silverstripe CMS recipe for all modules, optional and suggested",
    "homepage": "https://github.com/silverstripe/recipe-kitchen-sink",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.3",
        "silverstripe/recipe-plugin": "2.x-dev",
        "silverstripe/installer": "6.x-dev",
        "silverstripe/recipe-authoring-tools": "3.x-dev",
        "silverstripe/recipe-blog": "3.x-dev",
        "silverstripe/recipe-collaboration": "3.x-dev",
        "silverstripe/recipe-form-building": "3.x-dev",
        "silverstripe/recipe-reporting-tools": "3.x-dev",
        "silverstripe/recipe-services": "3.x-dev",
        "silverstripe/recipe-content-blocks": "4.x-dev",
        "silverstripe/auditor": "4.x-dev",
        "silverstripe/developer-docs": "6.x-dev",
        "silverstripe/environmentcheck": "4.x-dev",
        "silverstripe/hybridsessions": "4.x-dev",
        "silverstripe/registry": "4.x-dev",
        "silverstripe/totp-authenticator": "6.x-dev",
        "silverstripe/mfa": "6.x-dev",
        "silverstripe/crontask": "4.x-dev",
        "silverstripe/gridfieldqueuedexport": "4.x-dev",
        "silverstripe/ldap": "3.x-dev",
        "silverstripe/textextraction": "5.x-dev",
        "silverstripe/realme": "6.x-dev",
        "silverstripe/webauthn-authenticator": "6.x-dev",
        "silverstripe/subsites": "4.x-dev",
        "silverstripe/lumberjack": "4.x-dev",
        "silverstripe/staticpublishqueue": "7.x-dev",
        "dnadesign/silverstripe-elemental-userforms": "5.x-dev",
        "symbiote/silverstripe-multivaluefield": "7.x-dev",
        "symbiote/silverstripe-gridfieldextensions": "5.x-dev",
        "symbiote/silverstripe-queuedjobs": "6.x-dev",
        "colymba/gridfield-bulk-editing-tools": "5.x-dev",
        "tractorcow/silverstripe-fluent": "8.x-dev",
        "silverstripe/dynamodb": "6.x-dev",
        "silverstripe/linkfield": "dev-pulls/5/config-not-runtime as 5.x-dev",
        "silverstripe/graphql": "6.x-dev",
        "silverstripe/admin": "dev-pulls/3/new-admincontroller as 3.9999999.9999999.9999999-dev",
        "silverstripe/externallinks": "dev-pulls/4/new-admincontroller as 4.9999999.9999999.9999999-dev",
        "silverstripe/asset-admin": "dev-pulls/3/new-admincontroller as 3.9999999.9999999.9999999-dev",
        "dnadesign/silverstripe-elemental": "dev-pulls/6/new-admincontroller as 6.9999999.9999999.9999999-dev",
        "silverstripe/campaign-admin": "dev-pulls/3/new-admincontroller as 3.9999999.9999999.9999999-dev",
        "silverstripe/versioned-admin": "dev-pulls/3/new-admincontroller as 3.9999999.9999999.9999999-dev",
        "silverstripe/cms": "dev-pulls/6/new-admincontroller as 6.9999999.9999999.9999999-dev",
        "silverstripe/userforms": "dev-pulls/7/config-not-runtime as 7.9999999.9999999.9999999-dev"
    },
    "require-dev": {
        "silverstripe/frameworktest": "dev-pulls/2/new-admincontroller as 2.x-dev",
        "silverstripe/graphql-devtools": "2.x-dev",
        "silverstripe/recipe-testing": "^4",
        "mikey179/vfsstream": "^1.6"
    },
    "extra": {
        "project-files": [
            "app/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": {
        "silverstripe/admin": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-admin.git"
        },
        "silverstripe/externallinks": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-externallinks.git"
        },
        "silverstripe/asset-admin": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-asset-admin.git"
        },
        "dnadesign/silverstripe-elemental": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-elemental.git"
        },
        "silverstripe/frameworktest": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-frameworktest.git"
        },
        "silverstripe/campaign-admin": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-campaign-admin.git"
        },
        "silverstripe/versioned-admin": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-versioned-admin.git"
        },
        "silverstripe/cms": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-cms.git"
        },
        "silverstripe/linkfield": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-linkfield.git"
        },
        "silverstripe/userforms": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-userforms.git"
        }
    }
}
